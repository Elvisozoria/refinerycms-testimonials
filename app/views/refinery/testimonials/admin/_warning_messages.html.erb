<% if object.warnings.any? %>
  <div class="warningExplanation" id="warnings">
    <p class="warning_icon">Problems in the following fields:</p>
    <ul>
    <% if defined?(include_object_name) and include_object_name %>
      <% object.warnings.full_messages.each do |value| %>
        <li><%= value %></li>
      <% end %>
    <% else %>
      <% object.warnings.each do |key, value| %>
        <li><%= value %></li>
      <% end %>
    <% end %>
    </ul>
  </div>
<% end %>
